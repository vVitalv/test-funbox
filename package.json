{
  "name": "testfunbox",
  "version": "1.0.0",
  "main": "index.js",
  "author": "pepel",
  "license": "MIT",
  "scripts": {
    "dev": "yarn run watch:server & yarn run watch:client",
    "watch:server": "nodemon start.js",
    "watch:client": "webpack serve --config webpack.dev.js",
    "lint": "eslint './server/**/*.js' './client/**/*.js' './client/**/*.jsx'",
    "prettier": "prettier './server/**/*.js' './client/**/*.js' './client/**/*.jsx' --write --no-error-on-unmatched-pattern",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js --progress --profile",
    "start": "cross-env NODE_ENV=production && node start.js",
    "heroku-postbuild": "yarn run build"
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.17.9",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.5",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-webpack-plugin": "^3.1.1",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.3",
    "prettier": "^2.6.2",
    "sass-loader": "^12.6.0",
    "tailwindcss": "^3.0.24",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.7.3"
  }
}
